{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Serilog": {
        "Using":  [ "Serilog.Sinks.Console" ],
        "MinimumLevel": "Information",
        "WriteTo": [
            { "Name": "Console" }
        ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
    },
    "ConsoleUpdateWorkerConfiguration": {
        "UpdateIntervalMilliseconds": "10000"
    },
    "TwitterClientConfiguration": {
        "ConsumerKey":  "???",
        "ConsumerSecret": "???",
        "BearerToken": "???"
    }
}